    {
      "kanji": "割",
      "reading": "わり",
      "meaning": "tenth; 10% unit (used for percentages/discount rates)",
      "example": {
        "ja": "この店は二割引です。",
        "en": "This shop has a 20% discount.",
        "notes": [
          "X割 = X × 10%. (一割=10%, 二割=20%).",
          "Common pattern: 〜割引 (わりびき)."
        ]
      }
    },
    {
      "kanji": "一つ",
      "reading": "ひとつ",
      "meaning": "one (thing); one item (native 'つ' counting)",
      "example": {
        "ja": "りんごを一つください。",
        "en": "One apple, please.",
        "notes": [
          "Generic counter for items; very common in daily speech."
        ]
      }
    },
